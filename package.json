{
  "name": "Leaflet-Webpack",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "html-webpack-plugin": "^4.5.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.1",
    "vue-template-compiler": "^2.6.12",
    "webpack-cli": "^4.2.0"
  },
  "old-scripts": {
    "prebuild": "rm -r dist",
    "build": "webpack --mode=production && cp -v ./src/index.html ./dist/",
    "start": "webpack --mode=development && cp -v ./src/index.html ./dist/",
    "prestart": "rm -r dist",
    "poststart": "webpack serve"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config-nod=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "laravel-mix": "^5.0.9",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2"
  }
}
